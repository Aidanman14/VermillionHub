    Killaura = false;
    Auto_Vegeta = false;
    Auto_Strength = false;
    Auto_Stamina = false;
    LoopKillAll = false;
}

local Players_Table = {}
for i,v in pairs(game.Players:GetPlayers()) do
if v ~= game.Players.LocalPlayer then
table.insert(Players_Table, v.Name)
end
end

local player = game:GetService("Players").LocalPlayer
local character = player.Character or player.CharacterAdded:wait()
local vim = game:GetService("VirtualInputManager")
local Runservice = game:GetService("RunService")
local cam = workspace.CurrentCamera
local CoreGui = game:GetService("StarterGui")

local function Teleport_Bypass(pos, t, skip)
    local tween_s = game:GetService('TweenService')
    local tweeninfo = TweenInfo.new(t,Enum.EasingStyle.Linear)
 
    local player = game.Players.LocalPlayer
 
    if player.Character and player.Character:FindFirstChild('HumanoidRootPart') then
        local cf = pos
        local tp = {CFrame = cf}
        local a = tween_s:Create(player.Character.HumanoidRootPart,tweeninfo,tp)
        a:Play()
        if not skip then
            a.Completed:Wait()
        end
    end
 end

   local UIlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Aidanman14/Test_ui/main/PJMUI"))()
  
    local Win = UIlib:Window("Vermillion Hub", "DBFR")
    local Main = Win:Tab("Main")
    local Misc = Win:Tab("Misc")
    local Troll = Win:Tab("Trolling")
    local Credits = Win:Tab("Credits")
  
spawn(function()
      pcall(function()
          getgenv().UwU:Notify("Thanks for using Vermillion Hub!", "Script has been loaded!") --Notification example lmao
    end)
end)

Troll:Button("Delete Map", function()
    local args = {
        [1] = {
            [1] = "KiBlastHit",
            [2] = game:GetService("Workspace").Debris,
            [3] = CFrame.new(Vector3.new(0,0,0), Vector3.new(0,0,0))
        }
    }
    game:GetService("ReplicatedStorage").Remotes.KiBlasts:InvokeServer(unpack(args))
end)

Troll:Button("Kill All", function()
    for _,v in pairs (game:GetService("Players"):GetPlayers()) do
        if v~= game:GetService("Players").LocalPlayer then
     local args = {
         [1] = {
             [1] = "KiBlastHit",
             [2] = v.Character.Torso,
             [3] = CFrame.new(Vector3.new(0,0,0), Vector3.new(0,0,0))
         }
     }
     game:GetService("ReplicatedStorage").Remotes.KiBlasts:InvokeServer(unpack(args))
     end
     end
end)

Troll:Toggle("Loop Kill All", false, function(t)
    getgenv().config.LoopKillAll = t
end)

Troll:Dropdown("Choose A Player", Players_Table, function(t)
    SelectedPlayer = t
end)

Troll:Button("Kill Player", function()
    local args = {
        [1] = {
            [1] = "KiBlastHit",
            [2] = game:GetService("Players")[SelectedPlayer].Character.Torso,
            [3] = CFrame.new(Vector3.new(0,0,0), Vector3.new(0,0,0))
        }
    }
    game:GetService("ReplicatedStorage").Remotes.KiBlasts:InvokeServer(unpack(args))
    
end)

Misc:Button("Inf Stamina", function()
    game:GetService("Players").LocalPlayer.Data.Stamina.Value = math.huge
    game:GetService("Players").LocalPlayer.Data.MaxStamina.Value = math.huge
end)

Misc:Button("Inf Ki", function()
    game:GetService("Players").LocalPlayer.Data.Ki.Value = math.huge
    game:GetService("Players").LocalPlayer.Data.MaxKi.Value = math.huge
end)

Misc:Button("Fast Charge", function()
    game:GetService("Players").LocalPlayer.Data.ChargeSpeed.Value = 999
end)

