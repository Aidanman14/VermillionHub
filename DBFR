local v1 = true;
game.RunService.Stepped:Connect(function()
pcall(function()
if getgenv().config.Killaura and v1 then 
    v1 = false;
    for i,v in pairs(workspace:GetChildren()) do
        if v.Name ~= player.Name and v:FindFirstChild("Torso") and (player.Character:FindFirstChild("Torso").Position - v.Torso.Position).Magnitude <= 10 and v:WaitForChild("Humanoid").Health > 0 then
            Teleport_Bypass(CFrame.new(v.HumanoidRootPart.Position + Vector3.new(0,0,4)), 0, true)
            task.spawn(function()
                local args = {
                [1] = v,
                [2] = {
                    ["HitTime"] = 0.5,
                    ["Type"] = "Normal",
                    ["HitEffect"] = game:GetService("ReplicatedStorage").Resources.OtherEffects.NormalHitEffect2,
                    ["VictimCFrame"] = v.Torso.CFrame,
                    ["HurtAnimation"] = game:GetService("ReplicatedStorage").Resources.Animations.HurtAnimations.Hurt2,
                    ["Velocity"] = Vector3.new(0.01, 0.01, 0.01),
                    ["Sound"] = game:GetService("ReplicatedStorage").Resources.Sounds.Combat.NormalKick,
                    ["Damage"] = 1000
                }
                }
                game:GetService("ReplicatedStorage").Remotes.Damage4:InvokeServer(unpack(args))
            end)
        end
    end    
    v1 = true;
    end
end)
end)

local v2 = true;
game.RunService.Stepped:Connect(function()
pcall(function()
if getgenv().config.LoopKillAll and v2 then 
    v2 = false;
    for _,v in pairs (game:GetService("Players"):GetPlayers()) do
        if v~= game:GetService("Players").LocalPlayer then
     local args = {
         [1] = {
             [1] = "KiBlastHit",
             [2] = v.Character.Torso,
             [3] = CFrame.new(Vector3.new(0,0,0), Vector3.new(0,0,0))
         }
     }
     game:GetService("ReplicatedStorage").Remotes.KiBlasts:InvokeServer(unpack(args))
     end
     end  
     task.wait() 
    v2 = true;
    end
end)
end)
